{
  "name": "ricehalla",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "run-p db:up",
    "start": "run-p watch:*",
    "watch:client": "webpack -d -w",
    "watch:server": "nodemon -w ./src -i ./src/client -x \"DEBUG=graphile-build:warn babel-node -x .ts,.js ./src/server.ts\"",
    "watch:codegen": "gql-gen -w",
    "build": "NODE_ENV=production webpack -p",
    "db:up": "docker-compose up -d db",
    "postinstall": "typesync",
    "lint:js": "eslint --ext=.js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint './src/**/*.css'",
    "lint": "run-p lint:*",
    "typecheck": "tsc -p .",
    "test": "run-p lint:* typecheck"
  },
  "proxy": "http://localhost:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
